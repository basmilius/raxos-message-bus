{
    "name": "raxos/message-bus",
    "description": "A Message Bus implementation for Raxos.",
    "license": "mit",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "type": "library",
    "authors": [
        {
            "name": "Bas Milius",
            "email": "bas@mili.us"
        }
    ],
    "autoload": {
        "psr-4": {
            "Raxos\\MessageBus\\": "src/Raxos/MessageBus"
        }
    },
    "config": {
        "platform": {
            "php": "8.4"
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "../*",
            "symlink": true
        }
    ],
    "require": {
        "jetbrains/phpstorm-attributes": "^1.2",
        "php-amqplib/php-amqplib": "^v3.7",
        "raxos/foundation": "*",
        "raxos/terminal": "*"
    }
}
